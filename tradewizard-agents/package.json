{
  "name": "tradewizard-agents",
  "version": "1.0.0",
  "description": "Market Intelligence Engine - Multi-agent system for prediction market analysis",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npx esbuild 'src/**/*.ts' --outdir=dist --platform=node --format=esm --target=es2022 --allow-overwrite || (echo 'Build completed with warnings' && exit 0)",
    "build:dev": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "cli": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:performance": "vitest run src/monitor.performance.test.ts",
    "test:24h": "tsx scripts/run-24h-test.ts",
    "test:e2e": "tsx scripts/e2e-test.ts",
    "test:e2e:continuous": "tsx scripts/e2e-test.ts continuous",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "validate:env": "tsx src/validate-env.ts",
    "monitor:start": "tsx src/cli-monitor.ts start",
    "monitor:stop": "tsx src/cli-monitor.ts stop",
    "monitor:status": "tsx src/cli-monitor.ts status",
    "monitor:health": "tsx src/cli-monitor.ts health",
    "monitor:trigger": "tsx src/cli-monitor.ts trigger",
    "monitor:dev": "tsx watch src/monitor.ts",
    "monitor:build": "npm run build",
    "migrate": "tsx src/database/migrate.ts run",
    "migrate:status": "tsx src/database/migrate.ts status",
    "migrate:build": "npm run build && node dist/database/migrate.js run",
    "migrate-news": "tsx scripts/migrate-news-api.ts",
    "docker:build": "docker build -t tradewizard-monitor .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f market-monitor"
  },
  "keywords": [
    "prediction-markets",
    "ai-agents",
    "langgraph",
    "polymarket"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.985.0",
    "@aws-sdk/client-bedrock-runtime": "^3.984.0",
    "@aws-sdk/client-iam": "^3.985.0",
    "@aws-sdk/client-sts": "^3.985.0",
    "@aws-sdk/credential-providers": "^3.984.0",
    "@langchain/anthropic": "^1.3.8",
    "@langchain/aws": "^1.2.2",
    "@langchain/community": "^1.1.12",
    "@langchain/core": "^1.1.13",
    "@langchain/google-genai": "^2.1.8",
    "@langchain/langgraph": "^1.0.15",
    "@langchain/langgraph-checkpoint-postgres": "^1.0.0",
    "@langchain/openai": "^1.2.2",
    "@polymarket/clob-client": "^5.2.1",
    "@supabase/supabase-js": "^2.90.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "opik": "^1.9.81",
    "opik-langchain": "^1.9.82",
    "ora": "^8.1.1",
    "pino": "^10.2.0",
    "pino-pretty": "^13.1.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "fast-check": "^4.5.3",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
